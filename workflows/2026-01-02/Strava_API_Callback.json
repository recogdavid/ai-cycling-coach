    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "-- Insert or update based on Strava athlete ID\nINSERT INTO athletes (strava_athlete_id, name, strava_access_token, strava_refresh_token, token_expires_at, strava_scope)\nVALUES (\n  {{ $json.strava_athlete_id }},\n  '{{ $json.strava_athlete_name }}',\n  '{{ $json.strava_access_token }}',\n  '{{ $json.strava_refresh_token }}',\n  to_timestamp({{ $json.strava_expires_at }}),\n  NULLIF('{{ $json.strava_scope }}', 'null')\n)\nON CONFLICT (strava_athlete_id) \nDO UPDATE SET\n  strava_access_token = EXCLUDED.strava_access_token,\n  strava_refresh_token = EXCLUDED.strava_refresh_token,\n  token_expires_at = EXCLUDED.token_expires_at,\n  strava_scope = EXCLUDED.strava_scope\nRETURNING id, name, strava_athlete_id;",
        "options": {}
      },
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.6,
      "position": [
        -48,
        -64
      ],
      "id": "0226d44b-ea7b-4e10-8bde-bf474abd8db9",
