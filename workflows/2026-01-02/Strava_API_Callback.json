    {
      "parameters": {
        "jsCode": "return [{\n  json: {\n    athlete_id: $json.id,\n    athlete_name: $json.name,\n    redirect_url: \"https://dabosch.fit/onboarding.html?athlete_id=\" + $json.id + \"&strava_connected=true\",\n    set_cookie: true\n  }\n}];"
      },
